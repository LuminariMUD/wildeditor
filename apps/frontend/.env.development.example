# Development Environment Configuration
# Copy this to .env.local for local development
# This file is specifically for running against a local backend

# Backend API URL for local development
VITE_API_URL=http://localhost:8000/api

# API Key for destructive operations (use your local dev key)
VITE_WILDEDITOR_API_KEY=your_local_dev_api_key_here

# Supabase Configuration (development project)
VITE_SUPABASE_URL=your_dev_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_dev_supabase_anon_key

# Development Settings
VITE_ENABLE_DEBUG=true
VITE_MOCK_API=false
VITE_DISABLE_AUTH=false
