# Frontend Environment Configuration
# Luminari Wilderness Editor
# Copy this file to .env and fill in your actual values

# Backend API URL
# For LOCAL DEVELOPMENT ONLY: uncomment the line below
# VITE_API_URL=http://localhost:8000/api
# Production uses HTTPS by default: https://api.wildedit.luminarimud.com/api

# API Key for destructive operations (POST, PUT, DELETE)
# This key is required for creating, updating, or deleting wilderness objects
VITE_WILDEDITOR_API_KEY=your_api_key_here

# Supabase Configuration (for authentication)
# Get these values from your Supabase project dashboard
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Development Settings (optional)
VITE_ENABLE_DEBUG=true
VITE_MOCK_API=false

# Application Settings
VITE_APP_NAME=Luminari Wilderness Editor
VITE_ENVIRONMENT=development
VITE_ENABLE_ANALYTICS=false